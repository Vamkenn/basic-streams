{
  "name": "basic-streams",
  "version": "0.1.0",
  "description": "Experimental very basic reactive streams implementation for JavaScript",
  "main": "dist/index.js",
  "scripts": {
    "coverage": "nyc --require=babel-register tape test/*.js && nyc report --reporter=text-lcov > lcov.info",
    "tapec": "nodemon --watch test --watch src --exec \"tape -r babel-register test/*.js | tap-notify | faucet\"",
    "test": "eslint . && flow check && tape -r babel-register test/*.js | faucet",
    "build": "webpack"
  },
  "keywords": [
    "reactive",
    "reactive programming",
    "functional",
    "functional programming",
    "functional reactive programming",
    "frp",
    "streams",
    "stream",
    "observables",
    "observable"
  ],
  "author": "Roman Pominov <rpominov@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "6.3.26",
    "babel-eslint": "5.0.0-beta6",
    "babel-loader": "6.2.0",
    "babel-plugin-check-es2015-constants": "6.3.13",
    "babel-plugin-syntax-flow": "6.3.13",
    "babel-plugin-transform-es2015-arrow-functions": "6.3.13",
    "babel-plugin-transform-es2015-block-scoped-functions": "6.3.13",
    "babel-plugin-transform-es2015-block-scoping": "6.3.13",
    "babel-plugin-transform-es2015-computed-properties": "6.3.13",
    "babel-plugin-transform-es2015-destructuring": "6.3.15",
    "babel-plugin-transform-es2015-for-of": "6.3.13",
    "babel-plugin-transform-es2015-function-name": "6.3.21",
    "babel-plugin-transform-es2015-literals": "6.3.13",
    "babel-plugin-transform-es2015-modules-commonjs": "6.3.16",
    "babel-plugin-transform-es2015-parameters": "6.3.26",
    "babel-plugin-transform-es2015-shorthand-properties": "6.3.13",
    "babel-plugin-transform-es2015-spread": "6.3.14",
    "babel-plugin-transform-es2015-template-literals": "6.3.13",
    "babel-plugin-transform-es2015-typeof-symbol": "6.3.13",
    "babel-plugin-transform-flow-strip-types": "6.3.15",
    "babel-plugin-transform-object-rest-spread": "6.3.13",
    "babel-register": "6.3.13",
    "eslint": "1.10.3",
    "eslint-plugin-flow-vars": "0.1.3",
    "faucet": "0.0.1",
    "flow-bin": "0.20.1",
